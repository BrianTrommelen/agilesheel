<div class="container">
    <div class="row">
        <h2>View your Sales</h2>
    </div>

    <div class="row">

        <form asp-action="CheckSales" method="post">
            <div class="form-group">
                <label for="startTime" class="control-label">Starttime</label>
                <input id="startTime" name="startTime" class="form-control" type="date">
            </div>

            <div class="form-group">
                <label for="endTime" class="control-label">Endtime</label>
                <input id="endTime" name="endTime" class="form-control" type="date">
            </div>

            <input type="submit" value="Search" class="btn btn-info" />
        </form>

    </div>
</div>